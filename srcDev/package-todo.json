{
  "name": "vue-dlg",
  "version": "1.0.0-alpha",
  "description": "Vue Dialog",
  "author": "Andrey Tyurin <ectb@mail.ru>",
  "scripts": {
    "serve": "vue-cli-service serve --port 3000 srcDev/main.js",
    "serveBuild": "vue-cli-service serve --port 3000 srcDev/mainBuild.js",
    "build": "vue-cli-service build --target lib --name vue-dialog src/plugin.js && cp lib-build/vue-dialog.umd.min.js lib/vue-dialog.js",
    "buildReprot": "vue-cli-service build --report --target lib --name vue-dialog src/plugin.js",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages -d demo srcDev/main.js",
    "predeploy": "vue-cli-service build --mode demo srcDev/main.js"
  },
  "main": "./lib/vue-dialog.js",
  "files": [
    "lib",
    "src"
  ],
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-oploshka": "^1.0.7",
    "eslint-plugin-vue": "^8.4.1",
    "gh-pages": "^3.1.0"
  },
  "browserslist": [
    "last 2 chrome versions"
  ],
  "homepage": "https://oploshka.github.io/vue-dialog/",
  "keywords": [
    "vue",
    "vuejs",
    "vue-dialog",
    "vue-dlg",
    "dialog",
    "dialogs",
    "slim",
    "alert",
    "confirm",
    "prompt",
    "v-dlg",
    "v-dialog",
    "v-dialogs"
  ],
  "license": "MIT",
  "repository": "git@github.com:oploshka/vue-dialog.git"
}
